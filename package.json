    "umzug": "^3.8.2",
    "uuid": "^11.0.3",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@playwright/test": "^1.55.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/config": "^3.3.5",
    "@types/express": "^5.0.0",
    "@types/http-signature": "^1.4.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.9.0",
    "@types/nodemailer": "^6.4.16",
