    "activitypub-express": "^4.4.2",
    "axios": "^1.7.7",
    "config": "^3.3.12",
    "debug": "^4.3.7",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "flydrive": "^1.2.0",
    "handlebars": "^4.7.8",
    "http-errors": "^2.0.0",
    "http-signature": "^1.4.0",
    "i18next": "^25.0.2",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-fs-backend": "^2.6.0",
    "i18next-vue": "^5.2.0",
    "iso-639-1-dir": "^3.0.5",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.5.0",
    "multer": "^2.0.1",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
