{
  "name": "pavillion",
  "version": "0.0.0",
  "repository": "github:stephenhoward/pavilion",
  "license": "MIT",
  "type": "module",
  "private": true,
  "main": "src/server/app.ts",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "NODE_ENV=development tsx watch src/server/app.ts",
    "dev": "concurrently 'npm:dev:frontend' 'npm:dev:backend'",
    "start": "NODE_ENV=production tsx src/server/app.ts",
    "build": "npm run build:frontend",
    "build:frontend": "vite build",
    "preview": "vite preview",
    "test": "npx vitest run",
    "test:unit": "npx vitest run --exclude='**/integration/**'",
    "test:coverage": "npx vitest run --exclude='**/integration/**' --coverage",
    "test:integration": "npx vitest run integration",
    "test:watch": "npx vitest --ui",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:debug": "npx playwright test --debug",
    "lint": "eslint --ext .ts,.js,.vue src",
    "lint:fix": "eslint --ext .ts,.js,.vue src --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.828.0",
